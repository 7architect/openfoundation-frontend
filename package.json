{
  "name": "openfoundation-frontend",
  "version": "1.0.0",
  "description": "Open Foundation frontend",
  "author": "Александр Катков",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "nuxt": "2.12.2",
    "@nuxtjs/axios": "5.10.3",
    "@nuxtjs/pwa": "3.0.0-beta.20",
    "@nuxtjs/dotenv": "1.4.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "2.0.2",
    "@nuxtjs/eslint-module": "1.2.0",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-nuxt": "1.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "prettier": "1.19.1",
    "husky": "5.0.9",
    "lint-staged": "10.2.6",
    "@nuxtjs/stylelint-module": "3.2.2",
    "stylelint": "10.1.0",
    "@vue/test-utils": "1.0.3",
    "babel-jest": "24.9.0",
    "jest": "24.9.0",
    "vue-jest": "4.0.0-beta.2"
  }
}
